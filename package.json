{
  "name": "performance-decorators",
  "version": "3.0.0",
  "description": "A set of decorators to help with performance testing",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest"
  },
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./optimization": {
      "types": "./dist/optimization/index.d.ts",
      "import": "./dist/optimization/index.js"
    },
    "./debugging": {
      "types": "./dist/debugging/index.d.ts",
      "import": "./dist/debugging/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RyanMyrvold/Performance-Decorators.git"
  },
  "keywords": [
    "typescript",
    "monitoring",
    "debugging",
    "performance",
    "decorators",
    "testing",
    "optimization",
    "lazy-loading",
    "performance-analysis"
  ],
  "author": "Ryan Myrvold",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": ">=29.5.14",
    "@types/node": ">=22.9.4",
    "jest": ">=29.7.0",
    "ts-jest": ">=29.2.5",
    "typescript": ">=5.7.2"
  }
}
